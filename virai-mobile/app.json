{
  "expo": {
    "name": "Vira Flow",
    "slug": "vira-flow",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "viraflow",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1B18"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.semanur.viraflow"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#2D2926"
      },
      "package": "com.semanur.viraflow",
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "@sentry/react-native/expo"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "sentryDsn": "https://ba4d63635b2579b9a0cdd184edba3d68@o4510721655242752.ingest.de.sentry.io/4510721661272144",
      "router": {},
      "eas": {
        "projectId": "d261fa49-a200-4c85-b4cc-af9bd3863877"
      }
    }
  }
}
